<?xml version="1.0" encoding="utf-8"?>
<com.android.album2yc.view.SquareRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/food_title_image"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scaleType="fitXY" />

        <ImageView
            android:visibility="gone"
            android:id="@+id/food_title_image_0"
            android:layout_width="66dp"
            android:layout_height="56dp"
            android:scaleType="fitXY"
            android:layout_gravity="right"
            android:layout_marginRight="11dp"
            android:layout_marginTop="11dp" />
    </FrameLayout>
</com.android.album2yc.view.SquareRelativeLayout>
